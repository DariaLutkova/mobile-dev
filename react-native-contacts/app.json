{
  "expo": {
    "name": "RN Contacts App",
    "slug": "react-native-contacts",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.dariaglam.react-native-contacts",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.dariaglam.react_native_contacts",
      "versionCode": 1,
      "permissions": [
        "READ_CONTACTS",
        "READ_EXTERNAL_STORAGE",
        "READ_PHONE_STATE",
        "WRITE_EXTERNAL_STORAGE",
        "WRITE_CONTACTS"
    ]
    }
  }
}
